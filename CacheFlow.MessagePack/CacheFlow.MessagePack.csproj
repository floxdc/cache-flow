<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.1;netcoreapp2.2;netcoreapp3.0;netcoreapp3.1</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Flox DC</Company>
    <Authors>Kirill Taran</Authors>
    <PackageId>FloxDc.CacheFlow.MessagePack</PackageId>
    <Version>1.6.2</Version>
    <Product>FloxDc.CacheFlow.MessagePack</Product>
    <Description>MessagePack serializer for CacheFlow</Description>
    <RepositoryUrl>https://bitbucket.org/floxdc/cache-flow</RepositoryUrl>
    <PackageTags>CacheFlow MessagePack</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>Updated to NetCore 3.1</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FloxDc.CacheFlow" Version="1.6.2" />
    <PackageReference Include="MessagePack" Version="2.0.335" />
    <PackageReference Include="MessagePack.ImmutableCollection" Version="2.0.335" />
  </ItemGroup>

</Project>
